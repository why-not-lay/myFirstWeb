<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>FirstWeb</display-name>
  <servlet>
    <description></description>
    <display-name>Login</display-name>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.myFirstWeb.servlet.Login</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>Index</display-name>
    <servlet-name>Index</servlet-name>
    <servlet-class>com.myFirstWeb.servlet.Index</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>LogOut</display-name>
    <servlet-name>LogOut</servlet-name>
    <servlet-class>com.myFirstWeb.servlet.Logout</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>SignIn</display-name>
    <servlet-name>SignIn</servlet-name>
    <servlet-class>com.myFirstWeb.servlet.SignIn</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>Seller</display-name>
    <servlet-name>Seller</servlet-name>
    <servlet-class>com.myFirstWeb.servlet.Seller</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>AddProduct</display-name>
    <servlet-name>AddProduct</servlet-name>
    <servlet-class>com.myFirstWeb.servlet.AddProduct</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>RemoveProduct</display-name>
    <servlet-name>RemoveProduct</servlet-name>
    <servlet-class>com.myFirstWeb.servlet.RemoveProduct</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>User</display-name>
    <servlet-name>User</servlet-name>
    <servlet-class>com.myFirstWeb.servlet.Userpage</servlet-class>
  </servlet>
  
  

  <servlet-mapping>
    <servlet-name>Seller</servlet-name>
    <url-pattern>/seller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddProduct</servlet-name>
    <url-pattern>/seller/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RemoveProduct</servlet-name>
    <url-pattern>/seller/remove</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>User</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  

  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogOut</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SignIn</servlet-name>
    <url-pattern>/signin</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>
